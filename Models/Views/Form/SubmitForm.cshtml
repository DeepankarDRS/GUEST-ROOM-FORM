@model FormModel

<form asp-action="SubmitForm" method="post">
    <div class="form-group">
        <label asp-for="UserEmail"></label>
        <input asp-for="UserEmail" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Detail1"></label>
        <input asp-for="Detail1" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Detail2"></label>
        <input asp-for="Detail2" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Submit Form</button>
</form>
